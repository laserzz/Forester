generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DB_URL")
}

model greets {
    guild String @id
    welcomechannel String?
    welcomecontent String?
    goodbyechannel String?
    goodbyecontent String?
}

model confession {
    guild String @id
    channel String
}